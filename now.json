{
  "version": 2,
  "name": "Rebass",
  "builds": [
    {
      "src": "www/package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "public"
      }
    }
  ],
  "alias": [
    "rebassjs.org"
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/www/$1"
    }
  ],
  "public": true
}
